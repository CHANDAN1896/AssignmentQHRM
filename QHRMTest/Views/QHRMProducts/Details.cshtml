@model QHRMTest.Models.QHRMProducts
<!Doctype>
<html>
<head>
    <title> Product Details</title>
    <a href="/QHRMProducts/Index" class="btn btn-primary mb-3">Go Back</a>
</head>
<body>
    <h1>@ViewData["Title"] Product</h1>
    <form method="post">
        <div class="form-group">
            <label>Name</label>
            <input type="text" asp-for="Name" class="form-control" readonly/>
        </div>
        <div class="form-group">
            <label>Description</label>
            <textarea asp-for="Description" class="form-control" readonly></textarea>
        </div>
        <div class="form-group">
            <label>Price</label>
            <input type="number" asp-for="Price" class="form-control" step="0.01" readonly/>
        </div>

        <div class="form-group">
            <label>Created</label>
            <input type="text" asp-for="Created" class="form-control" step="0.01" readonly/>
        </div>
    </form>

</body>
</html>

